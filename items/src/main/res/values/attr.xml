<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BaseItem">
        <attr name="xy_icon" format="reference" />
        <attr name="xy_text" format="string" />
        <attr name="xy_text_color" format="color" />
        <attr name="xy_text_size" format="dimension" />
        <attr name="xy_subtext" format="string" />
        <attr name="xy_subtext_size" format="dimension" />
        <attr name="xy_subtext_color" format="color" />
    </declare-styleable>

    <declare-styleable name="ClickItem">
        <attr name="xy_click_icon" format="reference" />
    </declare-styleable>

    <declare-styleable name="SwitchItem">
        <attr name="xy_checked" format="boolean" />
    </declare-styleable>
</resources>